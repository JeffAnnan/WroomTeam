<h1 class="center">Modifier une Ã©curie</h1>
{{#each listeInfoEcurieSelect}}
  <form action="/ecurieModifie/{{this.ecunum}}" method="post">
      <label>Nom</label><input type="text" name="ecunom" value="{{this.ecunom}}"><br /><br />
      <label>Direcreur</label><input type="text" name="ecunomdir" value="{{this.ecunomdir}}"><br /><br />
      <label>Adresse du siege</label><textarea name="ecuadrsiege" rows="3" cols="50">{{this.ecuadrsiege}}</textarea><br /><br />
      <label>Points</label><input type="text" size="10" name="ecupoints" value="{{this.ecupoints}}"> pts<br /><br />
      <label>Pays</label>
      <select name="paynum">
{{#each ../listePaysMemeQueEcurieSelect}}
  {{#if this.estmeme}}
    <option value="{{this.paynum}}" selected>{{this.paynom}}</option>
  {{else}}
    <option value="{{this.paynum}}">{{this.paynom}}</option>
  {{/if}}
{{/each}}
      </select><br /><br />
    <div class="center"><input type="submit" value="Modifier"></div><br />
  </form>
{{/each}}
